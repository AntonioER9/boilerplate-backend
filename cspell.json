{
  "language": "en",
  "useGitignore": true,
  "dictionaries": ["en_US", "softwareTerms", "typescript", "filetypes", "npm", "project-words"],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./.cspell/project-words.txt",
      "addWords": true
    }
  ],
  "ignorePaths": ["node_modules/**", "dist/**", "database/seeders/**", "**/mocks"],
  "overrides": [
    {
      "filename": "**/*.md",
      "caseSensitive": false
    },
    {
      "filename": "src/**/*.ts",
      "languageSettings": [
        {
          "languageId": "*",
          "caseSensitive": false
        }
      ]
    }
  ]
}
