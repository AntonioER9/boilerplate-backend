{
  "name": "hc-boilerplate-backend",
  "version": "1.0.0",
  "description": "Boilerplate for backend projects",
  "author": "help-center",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "compile": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.json",
    "format": "prettier -c '**/*.ts'",
    "format:fix": "prettier -w '**/*.ts'",
    "lint": "eslint . --ext .ts --max-warnings 3",
    "spell": "cspell --config ./cspell.json --no-progress --show-suggestions --show-context '**/*.ts' '**/*.md'",
    "audit": "npm audit --audit-level=critical",
    "validate": "npm run format && npm run lint && npm run spell && npm run compile",
    "docker:start": "docker compose up -d",
    "docker:build": "docker compose build",
    "docker:restart": "docker compose restart",
    "docker:stop": "docker compose stop",
    "docker:down": "docker compose down",
    "docker:down:complete": "docker compose down --remove-orphans -v",
    "db:debug": "mikro-orm debug",
    "db:ci": "mikro-orm schema:fresh --seed --run",
    "db:create": "mikro-orm schema:fresh --run",
    "db:update": "mikro-orm schema:update --run",
    "db:drop": "mikro-orm schema:drop --run",
    "db:seed": "mikro-orm seeder:run",
    "start": "nest start -p tsconfig.json",
    "start:dev": "nest start --watch -p tsconfig.build.json",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch -p tsconfig.build.json",
    "start:prod": "node dist/main",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --coverageDirectory=\"./coverage\" --ci --reporters=jest-junit --watchAll=false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@cencosud-ds/cda-flags": "1.2.0",
    "@fastify/cors": "8.3.0",
    "@fastify/helmet": "10.1.1",
    "@fastify/static": "6.10.2",
    "@mikro-orm/cli": "5.7.12",
    "@mikro-orm/core": "5.7.12",
    "@mikro-orm/mysql": "5.7.12",
    "@mikro-orm/nestjs": "5.1.8",
    "@mikro-orm/postgresql": "5.7.12",
    "@mikro-orm/reflection": "5.7.12",
    "@mikro-orm/seeder": "5.7.12",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "2.3.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-fastify": "9.4.3",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "joi": "17.9.2",
    "newrelic": "10.3.0",
    "passport": "0.6.0",
    "passport-headerapikey": "1.2.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.7",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@tsconfig/node18-strictest-esm": "1.0.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/newrelic": "9.14.0",
    "@types/node": "18.13.0",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "cspell": "6.31.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-unicorn": "47.0.0",
    "git-validate": "2.2.4",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.orm.json",
    "configPaths": [
      "./src/config/options/orm.option.ts",
      "./dist/config/options/orm.option.js"
    ]
  },
  "pre-commit": [
    "format",
    "lint",
    "spell",
    "compile"
  ],
  "pre-push": [
    "build",
    "test:cov",
    "audit"
  ]
}
